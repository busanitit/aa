-color: #145b3a;\r\n  color: #fff;\r\n  font-size: 22px;\r\n  font-weight: 600;\r\n  width: 45vw;\r\n`;\r\n\r\nclass EditUserPresenter extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.state = {\r\n      id:\"\",\r\n      userName: \"\",\r\n      password: \"\",\r\n      phoneNum: \"\",\r\n      address: \"\",\r\n    };\r\n  }\r\n\r\n  componentDidMount() {\r\n    userApi\r\n      .fetchUserById(window.localStorage.getItem(\"id\"))\r\n      .then((res) => {\r\n        const {\r\n          data: { id, userID, userName, password, phoneNum, address },\r\n        } = res;\r\n        this.setState({\r\n          id,\r\n          userID,\r\n          userName,\r\n          password,\r\n          phoneNum,\r\n          address,\r\n        });\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  }\r\n\r\n  onChange = (e) => {\r\n    const {\r\n      target: { name, value },\r\n    } = e;\r\n    this.setState({\r\n      [name]: value,\r\n    });\r\n  };\r\n\r\n  editUser = (e) => {\r\n    e.preventDefault(); // form post 발동방지\r\n    const { id, userName, password, address, phoneNum } = this.state;\r\n    let user = {\r\n      id,\r\n      userName,\r\n      password,\r\n      address,\r\n      phoneNum,\r\n    };\r\n    userApi\r\n      .editUser(user)\r\n      .then((res) => {\r\n        // this.setState({})\r\n        alert(\"유저 수정 성공\");\r\n        this.props.history.push(\"/users\");\r\n      })\r\n      .catch((err) => {\r\n        console.error(\"editUser() 에러\", err);\r\n        console.log(user);\r\n        alert(\"유저 수정 실패, 관리자에게 문의하십시오\", err);\r\n      });\r\n  };\r\n\r\n  render() {\r\n    const { userID, userName, password, address, phoneNum } = this.state;\r\n    return (\r\n      <>\r\n        <Title>Edit USer</Title>\r\n        <SemiTitle>회원정보 수정</SemiTitle>\r\n        <Container>\r\n          <Form>\r\n            <InputBox>\r\n              <Input\r\n                placeholder='User ID'\r\n                readOnly\r\n                value={userID}\r\n                name='userID'\r\n              />\r\n            </InputBox>\r\n            <InputBox>\r\n              <Input\r\n                placeholder='Password'\r\n                onChange={this.onChange}\r\n                type='password'\r\n                value={password}\r\n                name='password'\r\n              />\r\n            </InputBox>\r\n            <InputBox>\r\n              <Input\r\n                placeholder='User Name'\r\n                onChange={this.onChange}\r\n                value={userName}\r\n                name='userName'\r\n              />\r\n            </InputBox>\r\n            <InputBox>\r\n              <Input\r\n                placeholder='Phone Number'\r\n                onChange={this.onChange}\r\n                value={phoneNum}\r\n                name='phoneNum'\r\n              />\r\n            </InputBox>\r\n            <InputBox>\r\n              <Input\r\n                placeholder='Address'\r\n                onChange={this.onChange}\r\n                value={address}\r\n                name='address'\r\n              />\r\n            </InputBox>\r\n            <InputBox>\r\n              <Button onClick={this.editUser}>Edit</Button>\r\n            </InputBox>\r\n          </Form>\r\n        </Container>\r\n      </>\r\n    );\r\n  }\r\n}\r\nexport default EditUserPresenter;\r\n","C:\\DEV\\Sources\\final-shopping-mall\\src\\Routes\\Login\\LoginContainer.js",[],"C:\\DEV\\Sources\\final-shopping-mall\\src\\Routes\\Contact\\ContactPresenter.js",[],"C:\\DEV\\Sources\\final-shopping-mall\\src\\api.js",[],"C:\\DEV\\Sources\\final-shopping-mall\\src\\Routes\\SignUp\\SignUpPresenter.js",[],"C:\\DEV\\Sources\\final-shopping-mall\\src\\Routes\\Users\\UsersPresenter.js",[],"C:\\DEV\\Sources\\final-shopping-mall\\src\\components\\LinkImageForHome.js",[],["174","175"],"C:\\DEV\\Sources\\final-shopping-mall\\src\\Footer\\Footer.js",["176","177","178","179","